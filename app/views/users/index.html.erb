<div class="container">
  <div class="row">
    <% if @query_text%>
      <h3>Search Results</h3>
    <% else %>
      <h3>Users</h3>
    <% end %>
    <div class="column">
      <table class="table">
        <tbody>
          <% @users.each do |user| %>
            <tr>
              <%= render user %>
            </tr>
          <% end %>
        </tbody>
      </table>
    </div>
  </div>
</div>
